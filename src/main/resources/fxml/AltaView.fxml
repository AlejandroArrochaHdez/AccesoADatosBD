      <TextField fx:id="precioText" GridPane.columnIndex="1" GridPane.rowIndex="3" />
      <CheckBox fx:id="comedorCheck" mnemonicParsing="false" GridPane.columnIndex="1" GridPane.rowIndex="4" />
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="5.0" GridPane.columnSpan="2" GridPane.rowIndex="5">
         <padding>
            <Insets left="5.0" />
         </padding>
         <children>
            <Button mnemonicParsing="false" onAction="#onAceptarAction" text="Aceptar" />
         </children>
      </HBox>
